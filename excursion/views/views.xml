        <record model="ir.ui.view" id="excursion.movementday_form">
            <field name="name">Movement Day</field>
            <field name="model">excursion.movementday</field>
            <field name="arch" type="xml">
                <form create="false">
                    <sheet>
                        <div class="header text-center">
                            <h1>Journal des mouvements -
                                <field nolabel="1" name="date_m"/>
                            </h1>
                        </div>
                        <div class="oe_button_box" name="button_box">


                            <button name="config_order_hotel" type="object" class="btn oe_stat_button" icon="fa-gears"
                                    string=" Configurer heure depart">
                            </button>

                            <button name="print_order" type="object" class="btn oe_stat_button" icon="fa-print"
                                    string=" Imprimer Ordre mission">
                            </button>
                            <button name="excursion.wizards_ajouter_camel_action" type="action" class="btn oe_stat_button" icon="fa-plus"
                                    string="ajout camel"></button>
                        </div>
                        <group col="2">
                            <group>
                                <field name="excursion_id"/>
                                <field name="guide_principale"/>
                                <field name="numero_guide_principale"/>
                                <field name="guide_doubleur"/>
                                <field name="numero_guide_doubleur"/>
                                <field name="chauffeur"/>
                            </group>
                            <group>

                                <field name="vehicule"/>
                                <field name="num_order"/>
                                <field name="nbr_adulte"/>
                                <field name="nbr_enfant"/>
                                <field name="nbr_bebe"/>
                                <field name="pax"/>

                            </group>


                        </group>
                        <notebook>
                            <page string="Tourists  Data">

                                <field nolabel="1" name="touriste_info">
                                    <!--domain="[('date','=',date_m),('excursion_id','=',excursion_id),('movementday_id','=',False)]"-->
                                    <tree editable="top">
                                        <field name="ticket_number"/>
                                        <field name="room_nbr"/>
                                        <field name="hotel_id"/>
                                        <field name="vendeur_id"/>
                                        <field name="adulte" readonly="1"/>
                                        <field name="enfant" readonly="1"/>
                                        <field name="inf" readonly="1"/>
                                        <field name="radulte"/>
                                        <field name="renfant"/>
                                        <field name="rinf"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Extra Camel">

                                <field nolabel="1" name="Camel_info">

                                    <tree editable="top">
                                        <field name="ticket_number"/>
                                        <field name="room_nbr"/>
                                        <field name="hotel_id"/>
                                        <field name="vendeur_id"/>
                                        <field name="adulte" readonly="1"/>
                                        <field name="enfant" readonly="1"/>
                                        <field name="inf" readonly="1"/>
                                        <field name="radulte"/>
                                        <field name="renfant"/>
                                        <field name="rinf"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="nombre 4*4">
                                <group>
                                    <field name="nbr_q_q"/>
                                </group>
                            </page>
                            <page string="Representatives info">
                                <field name="representatives_info">
                                    <tree>
                                        <field name="representative_id"/>
                                        <field name="nbr_pax"/>
                                    </tree>
                                </field>

                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread" options="{&quot;thread_level&quot;: 1}"/>
                    </div>
                </form>
            </field>
        </record>

